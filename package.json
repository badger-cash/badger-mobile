  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "24.9.0",
    "babel-plugin-reflow": "^0.4.0",
    "eslint": "^6.4.0",
    "eslint-config-universe": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.5",
